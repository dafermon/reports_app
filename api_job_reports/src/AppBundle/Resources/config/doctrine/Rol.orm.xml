<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Rol" table="rol">
    <indexes>
      <index name="ID" columns="ID"/>
    </indexes>
    <id name="idTabla" type="integer" column="id_tabla">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="rol" type="string" column="ROL" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="id" target-entity="Contacto" fetch="LAZY">
      <join-columns>
        <join-column name="ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
