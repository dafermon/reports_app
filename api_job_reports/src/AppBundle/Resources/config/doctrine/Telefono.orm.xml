<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Telefono" table="telefono">
    <indexes>
      <index name="FK_telefono_usuario" columns="usuario"/>
    </indexes>
    <id name="idTelefono" type="bigint" column="id_telefono">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="numero" type="string" column="numero" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="usuario" target-entity="Usuario" fetch="LAZY">
      <join-columns>
        <join-column name="usuario" referenced-column-name="id_usuario"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
