<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Contacto" table="contacto">
    <id name="id" type="bigint" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="contacto" type="string" column="CONTACTO" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="clave" type="string" column="CLAVE" length="256" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="encriptada" type="string" column="ENCRIPTADA" length="256" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="habilitado" type="smallint" column="HABILITADO" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
