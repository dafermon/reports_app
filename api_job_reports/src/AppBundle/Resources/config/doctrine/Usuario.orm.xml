<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Usuario" table="usuario">
    <id name="idUsuario" type="string" column="id_usuario" length="32">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="apellidoMaterno" type="string" column="apellido_materno" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellidoPaterno" type="string" column="apellido_paterno" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="clave" type="string" column="clave" length="256" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombrePrimero" type="string" column="nombre_primero" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombreSegundo" type="string" column="nombre_segundo" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
